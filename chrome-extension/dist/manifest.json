{
  "name": "Vim Forever",
  "description": "Allows for development in a vim buffer that will paste to a codemirror editor",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "/popup.html"
  },
  "permissions": [
    "storage",
    "activeTab",
    "<all_urls>"
  ],
  "web_accessible_resources": [
    "/inject.js"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' chrome-extension://**/*;object-src 'self';"
}